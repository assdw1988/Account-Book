</servlet>
<servlet-mapping>
   <servlet-name>ajaxComm</servlet-name>
   <url-pattern>/ajaxComm</url-pattern>
</servlet-mapping>

<servlet>
   <servlet-name>mainPage</servlet-name>
   <servlet-class>jap.com.web003</servlet-class>
</servlet>
<servlet-mapping>
   <servlet-name>mainPage</servlet-name>
   <url-pattern>/mainPage</url-pattern>
</servlet-mapping>

<servlet>
   <servlet-name>actionP1</servlet-name>
   <servlet-class>jap.com.web004</servlet-class>
</servlet>
